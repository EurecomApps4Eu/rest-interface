@base <http://semweb.mmlab.be/ns/apps4X#>.

@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix owl: <http://www.w3.org/2002/07/owl#>.
@prefix foaf: <http://xmlns.com/foaf/0.1/>.
@prefix odapps: <http://semweb.mmlab.be/ns/odapps#>.
@prefix apps4X: <http://semweb.mmlab.be/ns/apps4X#>.
@prefix schema: <http://schema.org/> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix xsd:<http://www.w3.org/2001/XMLSchema#> .
@prefix lode: <http://linkedevents.org/ontology/> .
@prefix skos:<http://www.w3.org/2004/02/skos/core#>.

<{{rootUrl}}/rdf/event/{{_id}}>
  rdf:type apps4X:Event;
  dct:title "{{title}}";
  dct:description """{{text}}""";
  schema:startDate "{{startDateF}}"^^xsd:date;
  schema:endDate "{{endDateF}}"^^xsd:date;
  dct:spatial "{{location}}";
  odapps:theme "{{theme}}";
  apps4X:firstPrize "{{firstPrize}}";
  apps4X:secondPrize "{{secondPrize}}";
  apps4X:thirdPrize "{{thirdPrize}}";

  {{#each firstPrizeWinners}}
    apps4X:firstPrizeWinner <{{../rootUrl}}/rdf/application/{{this._id}}>;
  {{/each}}
  {{#each secondPrizeWinners}}
    apps4X:secondPrizeWinner <{{../rootUrl}}/rdf/application/{{this._id}}>;
  {{/each}}
  {{#each thirdPrizeWinners}}
    apps4X:thirdPrizeWinner <{{../rootUrl}}/rdf/application/{{this._id}}>;
  {{/each}}


  apps4X:registration "{{registrationLink}}";
  {{#each connectedApps}}apps4X:connectedApp <{{../rootUrl}}/rdf/application/{{this._id}}>;
  {{/each}}

  {{#each contacts}}
    apps4X:contactPerson [foaf:name "{{this.name}}"; foaf:mbox "{{this.email}}"; foaf:phone "{{this.phone}}"];
  {{/each}}

  {{#each organizers}}
    apps4X:organizer <{{this.url}}>;
  {{/each}}

  {{#each sponsors}}
    apps4X:sponsor <{{this.url}}>;
  {{/each}}

  {{#each juryMembers}}
    apps4X:juryMember [foaf:name "{{this.name}}"; foaf:mbox "{{this.email}}"];
  {{/each}}
  .
